        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="11:30 - 12:30"
        android:id="@+id/textView6"
        android:layout_below="@+id/view3"
